</listeners>

	<test thread-count="5" name="App Test">
		<classes>      
			<class name="udemyTutorial.AppTest"/>
		</classes>
	</test>
	<test thread-count="5" name="Validate Title Test">
		<classes>
			<class name="udemyTutorial.ValidateTitle"/>
